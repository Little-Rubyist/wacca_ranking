# encoding: utf-8
create_table "songs", force: :cascade, options: "ENGINE=InnoDB DEFAULT CHARSET=utf8" do |t|
  t.string :title
  t.string :ruby
  t.integer :song_id
  t.boolean :can_play_offline
  t.integer :genre
  t.integer :diff_type
  t.float :difficulty
  t.datetime "updated_at", precision: 6, null: false
  t.datetime "created_at", precision: 6, null: false
end
